{
  "name": "lunav2",
  "productName": "lunav2",
  "version": "1.0.0",
  "description": "lunav2",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development && cross-env START_HOT=1 npm run start-renderer-dev",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r @babel/register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src"
  },
  "author": {
    "name": "rvpanoz",
    "email": "rvpanoz@gmail.com",
    "url": "https://github.com/rvpanoz"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/rvpanoz/luna/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "redux",
    "sass",
    "webpack",
    "hot",
    "reload"
  ],
  "homepage": "https://github.com/rvpanoz/luna#readme",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "lunav2"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "lunav2"
      }
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "electron-compile": "^6.4.3",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^2.0.0",
    "q": "^1.5.1",
    "ramda": "^0.26.0",
    "react": "^16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-observable": "^1.0.0",
    "redux-react-hook": "^2.0.0",
    "request-promise-native": "^1.0.5",
    "rxjs": "^6.3.2",
    "rxjs-compat": "^6.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/plugin-transform-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.0-beta.6",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-import": "^1.11.0",
