    "moment": "^2.19.1",
    "q": "^1.5.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-poop": "^1.0.0",
    "react-redux": "^5.0.5",
